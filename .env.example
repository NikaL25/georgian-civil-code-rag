# Groq API (required for LLM)
GROQ_API_KEY=your_groq_api_key_here

# OpenAI API (required for fallback embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# PDF Path (download from Matsne: https://www.matsne.gov.ge/ka/document/download/31702/134/ka/pdf)
MATSNE_PDF_PATH=./matsne_civil_code.pdf

# Groq Model (optional; defaults to meta-llama/llama-4-maverick-17b-128e-instruct)
GROQ_MODEL_NAME=meta-llama/llama-4-maverick-17b-128e-instruct

# AstraDB (optional; for cloud vector store; use FAISS for local)
ASTRA_DB_APPLICATION_TOKEN=your_astra_token_here
ASTRA_DB_API_ENDPOINT=your_astra_endpoint_here
ASTRA_DB_COLLECTION=agentragcoll